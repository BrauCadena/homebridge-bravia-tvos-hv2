    "node": ">=16.0.0",
    "homebridge": ">02.0.0"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "@seydx/bravia": "^2.0.0-beta.21",
    "fs-extra": "^10.0.0"
