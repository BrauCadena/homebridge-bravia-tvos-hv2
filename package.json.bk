  "description": "Custom version of Bravia plugin for Homebridge v2",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeydX/homebridge-bravia-tvos.git"
  },
  "funding": [
    {
      "type": "paypal",
